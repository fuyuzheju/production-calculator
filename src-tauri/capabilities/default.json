{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "dialog:default",
    {
        "identifier": "fs:allow-app-read",
        "allow": [{"path": "/**/*"}]
    },
    {
        "identifier": "fs:allow-app-write",
        "allow": [{"path": "/**/*"}]
    }
  ]
}